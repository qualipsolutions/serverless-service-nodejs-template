{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "func-names": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "tabWidth": 2,
        "semi": true,
        "singleQuote": true,
        "printWidth": 80,
        "endOfLine": "auto"
      }
    ],
    "import/no-anonymous-default-export": 0
  },
  "plugins": ["prettier"]
}
